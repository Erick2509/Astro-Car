<style>
    .slider{
        height: 100px;
        margin: 50px auto;
        overflow: hidden;
        position: relative;
        width: auto;
    }
    .slider .slide-track{
        animation: scroll 5s linear infinite;
        display: flex;
        width: calc(250px * 14);
    }
    .slider .slide{
        height: 100px;
        display: flex;
        justify-content: center;
        width: 250px;
        opacity: .5;
    }
    
    @keyframes scroll{
        0%{
            transform: translateX(0);
        }
        100%{
            transform: translateX(calc(-250px * 7));
        }

    }

</style>